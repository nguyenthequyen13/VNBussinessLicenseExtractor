{
  "manifest_version": 3,
  "name": "Trích Xuất Đăng Ký Kinh Doanh (Gemini)",
  "version": "1.3",
  "description": "Extension trích xuất thông tin từ ảnh/PDF ĐKKD Việt Nam và điền vào form bất kỳ.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Trích Xuất ĐKKD",
    "default_icon": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}