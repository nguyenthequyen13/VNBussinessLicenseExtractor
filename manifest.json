{
  "manifest_version": 3,
  "name": "Trích Xuất Đăng Ký Kinh Doanh (Gemini)",
  "version": "1.3",
  "description": "Extension trích xuất thông tin từ ảnh/PDF ĐKKD Việt Nam và điền vào form bất kỳ.",
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Trích Xuất ĐKKD",
    "default_icon": "icon.svg"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}